{
    "env": {
        "jest": true,
        "commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": ["warn", { "argsIgnorePattern": "lazy" }],
        "no-extra-semi": 1
    }
}
